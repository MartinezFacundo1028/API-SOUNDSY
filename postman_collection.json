          "request": {
            "method": "GET",
            "url": {
              "raw": "{{base_url}}/reviews?serviceId=service_123",
              "host": ["{{base_url}}"],
              "path": ["reviews"],
              "query": [
                {
                  "key": "serviceId",
                  "value": "service_123"
                }
              ]
            }
          }
        },
        {
          "name": "Get Review by ID",
          "request": {
            "method": "GET",
