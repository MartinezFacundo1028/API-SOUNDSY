        {
          "name": "Create Review",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"orderId\": \"order_123\",\n  \"rating\": 5,\n  \"comment\": \"Excelente trabajo, muy profesional\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/reviews",
              "host": ["{{base_url}}"],
              "path": ["reviews"]
            }
          }
        },
        {
          "name": "Get User Reviews",
          "request": {
            "method": "GET",
